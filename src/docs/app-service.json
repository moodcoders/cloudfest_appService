    "securityDefinitions": {
        "JWT": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    },
    "definitions": {
        "Order": {
            "type": "object",
            "properties": {
                "location": {
                    "type": "string"
                },
                "serviceType": {
                    "type": "string"
                },
                "schedule": {
                    "type": "string",
                    "format": "date"
                },
                "isCompleted": {
                    "type": "boolean"
                },
                "handyman": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "rating": {
                            "type": "number"
                        },
                        "experience": {
                            "type": "number"
                        },
                        "profile": {
                            "type": "number"
                        }
                    }
                },
                "userId": {
                    "type": "string",
                    "default": "623b7de9686da70164931c7c"
                },
                "isPaid": {
                    "type": "boolean"
                },
                "paymenType": {
                    "type": "string"
                },
                "rating": {
                    "type": "object",
                    "properties": {
                        "feedback": {
                            "type": "string"
                        },
                        "rating": {
                            "type": "number"
                        }
                    }
                },
                "xml": {
                    "name": "Order"
                }
            }
        },
        "Get": {
            "type": "object",
            "properties": {
                "userId": {
                    "type": "string"
                }
            }
        },
        "Review": {
            "type": "object",
            "properties": {
                "rating": {
                    "type": "object",
                    "properties": {
                        "feedback": {
                            "type": "string"
                        },
                        "rating": {
                            "type": "number"
                        }
                    }
                },
                "id": {
                    "type": "string"
                }
            }
        },
        "Service":{
            "type": "object",
            "properties" :{
                "name": {
                    "type" :"string"
                },
                "isDisabled": {
                    "type": "boolean",
                    "default": false
                },
                "imgUrl": {
                    "type" :"string"
                },
                "description": {
                    "type" :"string"
                },
                "price":  {
                    "type" :"number"
                }
            }
        },
        "Category": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                }
            },
            "xml": {
                "name": "Category"
            }
        },
        "User": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "username": {
                    "type": "string"
                },
                "firstName": {
                    "type": "string"
                },
                "lastName": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                },
                "userStatus": {
                    "type": "integer",
                    "format": "int32",
                    "description": "User Status"
                }
            },
            "xml": {
                "name": "User"
            }
        },
        "Tag": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                }
            },
            "xml": {
                "name": "Tag"
            }
        },
        "Pet": {
            "type": "object",
            "required": [
                "name",
                "photoUrls"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "category": {
                    "$ref": "#/definitions/Category"
                },
                "name": {
                    "type": "string",
                    "example": "doggie"
                },
                "photoUrls": {
                    "type": "array",
                    "xml": {
                        "name": "photoUrl",
                        "wrapped": true
                    },
                    "items": {
                        "type": "string"
                    }
                },
                "tags": {
                    "type": "array",
                    "xml": {
                        "name": "tag",
                        "wrapped": true
                    },
                    "items": {
                        "$ref": "#/definitions/Tag"
                    }
                },
                "status": {
                    "type": "string",
                    "description": "pet status in the store",
                    "enum": [
                        "available",
                        "pending",
                        "sold"
                    ]
                }
            },
            "xml": {
                "name": "Pet"
            }
        },
        "ApiResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "type": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}