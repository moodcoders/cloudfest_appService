{
    "swagger": "2.0",
    "info": {
        "description": "App service will be responsible for providing db services to the front-ed",
        "version": "1.0.0",
        "title": "Handyman App Service",
        "contact": {
            "email": "osamabinjunaid36@gmail.com"
        }
    },
    "host": "localhost:5000",
    "basePath": "/v1",
    "tags": [
        {
            "name": "Booking",
            "description": "Access to Handyman bookings"
        },
        {
            "name": "Review",
            "description": "Access to Handyman reviews"
        },
        {
            "name": "Data",
            "description": "Access application specific static data"
        }
    ],
    "schemes": [
        "https",
        "http"
    ],
    "paths": {
        "/booking": {
            "post": {
                "tags": [
                    "Booking"
                ],
                "summary": "create a booking",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "patch": {
                "tags": [
                    "Booking"
                ],
                "summary": "update an existing booking",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "get": {
                "tags": [
                    "Booking"
                ],
                "summary": "get details of a booking",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Booking"
                ],
                "summary": "delete a booking",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            }
        },
        "/bookings": {
            "get": {
                "tags": [
                    "Booking"
                ],
                "summary": "get a list of a bookings",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            }
        },
        "/review": {
            "post": {
                "tags": [
                    "Review"
                ],
                "summary": "create a review",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "patch": {
                "tags": [
                    "Review"
                ],
                "summary": "update an existing review",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "get": {
                "tags": [
                    "Review"
                ],
                "summary": "get details of a review",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            },
            "delete": {
                "tags": [
                    "Review"
                ],
                "summary": "delete a review",
                "security": [
                    {
                        "JWT": []
                    }
                ]
            }
        },
        "/data/services": {
            "get": {
                "tags": [
                    "Data"
                ],
                "summary": "List of services that are supported"
            }
        }
    },
    "securityDefinitions": {
        "JWT": {
            "type": "apiKey",
            "name": "Authorization",
            "in": "header"
        }
    },
    "definitions": {
        "Order": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "petId": {
                    "type": "integer",
                    "format": "int64"
                },
                "quantity": {
                    "type": "integer",
                    "format": "int32"
                },
                "shipDate": {
                    "type": "string",
                    "format": "date-time"
                },
                "status": {
                    "type": "string",
                    "description": "Order Status",
                    "enum": [
                        "placed",
                        "approved",
                        "delivered"
                    ]
                },
                "complete": {
                    "type": "boolean",
                    "default": false
                }
            },
            "xml": {
                "name": "Order"
            }
        },
        "Category": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                }
            },
            "xml": {
                "name": "Category"
            }
        },
        "User": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "username": {
                    "type": "string"
                },
                "firstName": {
                    "type": "string"
                },
                "lastName": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                },
                "userStatus": {
                    "type": "integer",
                    "format": "int32",
                    "description": "User Status"
                }
            },
            "xml": {
                "name": "User"
            }
        },
        "Tag": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "name": {
                    "type": "string"
                }
            },
            "xml": {
                "name": "Tag"
            }
        },
        "Pet": {
            "type": "object",
            "required": [
                "name",
                "photoUrls"
            ],
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "category": {
                    "$ref": "#/definitions/Category"
                },
                "name": {
                    "type": "string",
                    "example": "doggie"
                },
                "photoUrls": {
                    "type": "array",
                    "xml": {
                        "name": "photoUrl",
                        "wrapped": true
                    },
                    "items": {
                        "type": "string"
                    }
                },
                "tags": {
                    "type": "array",
                    "xml": {
                        "name": "tag",
                        "wrapped": true
                    },
                    "items": {
                        "$ref": "#/definitions/Tag"
                    }
                },
                "status": {
                    "type": "string",
                    "description": "pet status in the store",
                    "enum": [
                        "available",
                        "pending",
                        "sold"
                    ]
                }
            },
            "xml": {
                "name": "Pet"
            }
        },
        "ApiResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32"
                },
                "type": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                }
            }
        }
    }
}